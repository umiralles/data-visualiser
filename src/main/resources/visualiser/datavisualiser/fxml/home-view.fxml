<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0"
          prefWidth="1116.0" styleClass="scene-grid" stylesheets="@../data-vis.css"
          xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="visualiser.datavisualiser.controllers.HomeController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="45.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="40.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" percentHeight="35.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" percentHeight="13.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Label styleClass="home-title" text="Data Visualiser" GridPane.halignment="RIGHT" GridPane.rowIndex="1"
           GridPane.valignment="TOP">
        <GridPane.margin>
          <Insets top="10.0"/>
        </GridPane.margin>
    </Label>
    <VBox prefHeight="200.0" prefWidth="100.0" styleClass="home-menu-vbox" GridPane.columnIndex="1"
          GridPane.rowIndex="1" GridPane.rowSpan="2">
        <children>
          <Button mnemonicParsing="false" prefHeight="36.0" styleClass="scene-menu-button" text="Import Database">
              <opaqueInsets>
                  <Insets left="10.0"/>
              </opaqueInsets>
              <VBox.margin>
                  <Insets/>
              </VBox.margin>
          </Button>
          <Button mnemonicParsing="false" onAction="#onDataConnectButtonClick" prefHeight="36.0"
                  styleClass="scene-menu-button" text="Connect to Database">
              <VBox.margin>
                  <Insets/>
              </VBox.margin>
          </Button>
          <Button mnemonicParsing="false" prefHeight="36.0" styleClass="scene-menu-button" text="Button">
              <VBox.margin>
                  <Insets/>
              </VBox.margin>
          </Button>
        </children>
        <GridPane.margin>
          <Insets left="40.0"/>
        </GridPane.margin>
    </VBox>
</GridPane>
